{
  "name": "spotfm",
  "version": "1.0",
  "description": "Spotify player for last.fm pages",
  "permissions": ["http://ws.spotify.com/*", "http://localhost:9999/*"],
  "content_scripts": [
    {
      "matches": ["http://www.last.fm/*"],
      "js": [
        "extension-core.js",
        "extension-chrome.js",
        "mootools-core-1.3-full-nocompat-yc.js",
        "core.js",
        "lastfm.js"],
      "css": ["lastfm.css"]
    }
  ],
  "background_page": "background.html"
  }

